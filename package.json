{
  "name": "splice",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:webpack": "webpack",
    "build:sass": "node-sass splice_ui/main.scss -o www --importer build_scripts/sass-importer.js",
    "start": "npm-run-all --parallel server watch:*",
    "watch:static": "cpx \"splice_ui/static/**/*\" www -w",
